{
  "name": "aquatracking-server",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:type-check": "tsc --noEmit",
    "build:build": "esbuild src/app.ts --bundle --packages=external --platform=node --format=cjs --outdir=dist --sourcemap",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.7",
    "sequelize": "^6.29.0",
    "sharp": "^0.30.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.9",
    "esbuild": "^0.19.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
